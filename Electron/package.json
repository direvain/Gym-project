{
    "name": "gym",
    "version": "1.0.0",
    "main": "main.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "electronmon .",
        "build-react": "cd my-gym && npm run build",
        "build-electron": "electron-builder",
        "package": "npm run build-react && npm run build-electron",
        "package-win": "npm run build-react && electron-builder --win",
        "postinstall": "electron-builder install-app-deps"
    },
    "author": "",
    "license": "ISC",
    "description": "",
    "dependencies": {
        "electronmon": "^2.0.3",
        "fs": "0.0.1-security",
        "path": "^0.12.7",
        "react-hook-form": "^7.54.2",
        "react-router-dom": "^7.2.0",
        "wait-on": "^8.0.3",
        "yup": "^1.6.1"
    },
    "build": {
    "appId": "com.yourapp.id",
    "productName": "City Gym",
    "files": [
        "**/*",
        "!my-gym/**/*",
        "!backend/**/*",
        "!**/node_modules/**/*"
    ],
    "extraResources": [
        {
        "from": "my-gym/build",
        "to": "build"
        },
        {
        "from": "backend",
        "to": "resources/backend"
        },
        {
        "from": "assets",
        "to": "assets"
        }
    ],
    "win": {
        "icon": "assets/city_Gym_Icon.ico",
        "target": [
        "nsis"
        ]
    }
    },
    "devDependencies": {
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "electron": "^34.2.0",
        "electron-builder": "^25.1.8",
        "sass": "^1.85.1"
    }
    }